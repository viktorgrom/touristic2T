<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ukrainetouristic">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/misze"
        android:label="MapsApp"
        android:roundIcon="@drawable/misze"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="Region_menu"/>
        <activity android:name=".PrePlaceInfoActivity" />
        <activity android:name=".PrePlaceInfoActivityZamku" />
        <activity android:name=".PrePlaceInfoActivityPlaces" />
        <activity android:name=".PrePlaceInfoActivityPamPrurodu" />
        <activity android:name=".PrePlaceInfoActivityZikaviMiszia" />
        <activity android:name=".PrePlaceInfoActivityReligiini" />
        <activity android:name=".PrePlaceInfoActivityParku" />
        <activity android:name=".PrePlaceInfoActivitySadubu" />
        <activity android:name=".PrePlaceInfoActivityZikaviMista" />
        <activity android:name=".Category_menu" />
        <activity android:name=".placeInfo.Place_info_activity" />
        <activity android:name=".map.MapActivity" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/map_key" />

        <activity android:name=".MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>